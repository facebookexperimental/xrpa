{
  "name": "@xrpa/xred-sensor-input",
  "version": "1.14.0",
  "description": "SensorInput interface",
  "main": "lib/index.js",
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "license": "Apache-2.0",
  "config": {
    "repo_path": "arvr/libraries/xred/xrpa/modules/xred-sensor-input"
  },
  "scripts": {
    "preinstall": "(node -e \"process.exit(process.platform === 'win32' ? 0 : 1)\") && ((edenfsctl info . && eden redirect add $npm_package_config_repo_path/node_modules symlink) || echo no eden found) || ((edenfsctl info . && eden redirect add $npm_package_config_repo_path/node_modules bind) || echo no eden found)",
    "build": "tsc",
    "lint": "eslint --ext .ts,.tsx ./src",
    "oss-publish": "tsc && ts-node ./scripts/publish.ts",
    "Aria": "ts-node ./scripts/AriaStandalone.ts",
    "Camera": "ts-node ./scripts/CameraStandalone.ts",
    "Aria:codegen": "ts-node ./scripts/AriaStandalone.ts --codegen-only",
    "Aria:build-only": "ts-node ./scripts/AriaStandalone.ts --build-only",
    "Aria:run-only": "ts-node ./scripts/AriaStandalone.ts --run-only",
    "Aria:codegen-and-build": "ts-node ./scripts/AriaStandalone.ts --codegen-and-build",
    "Camera:codegen": "ts-node ./scripts/CameraStandalone.ts --codegen-only",
    "Camera:build-only": "ts-node ./scripts/CameraStandalone.ts --build-only",
    "Camera:run-only": "ts-node ./scripts/CameraStandalone.ts --run-only",
    "Camera:codegen-and-build": "ts-node ./scripts/CameraStandalone.ts --codegen-and-build"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@xrpa/xred-perception-services": "^1.14.0",
    "@xrpa/xrpa-orchestrator": "^1.14.0",
    "eslint": "8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "ts-node": "^10.9.1",
    "xrpa-internal-scripts": "^1.14.0"
  },
  "peerDependencies": {
    "@xrpa/xred-perception-services": "^1.14.0",
    "@xrpa/xrpa-orchestrator": "^1.14.0"
  }
}
