{
  "name": "@xrpa/xred-perception-services",
  "version": "1.14.0",
  "description": "Machine-perception service modules",
  "main": "lib/index.js",
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc",
    "codegen": "yarn ImageSelector:codegen && yarn LlmHub:codegen && yarn ObjectRecognition:codegen",
    "build": "yarn ImageSelector:build && yarn LlmHub:build && yarn ObjectRecognition:build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "oss-publish": "tsc && ts-node ./scripts/publish.ts",
    "ImageSelector": "ts-node ./scripts/ImageSelectorStandalone.ts --codegen --run",
    "LlmHub": "ts-node ./scripts/LlmHubStandalone.ts --codegen --run",
    "ObjectRecognition": "ts-node ./scripts/ObjectRecognitionStandalone.ts --codegen --run",
    "ImageSelector:codegen": "ts-node ./scripts/ImageSelectorStandalone.ts --codegen",
    "ImageSelector:build": "ts-node ./scripts/ImageSelectorStandalone.ts --codegen --build",
    "ImageSelector:run-only": "ts-node ./scripts/ImageSelectorStandalone.ts --run",
    "LlmHub:codegen": "ts-node ./scripts/LlmHubStandalone.ts --codegen",
    "LlmHub:build": "ts-node ./scripts/LlmHubStandalone.ts --codegen --build",
    "LlmHub:run-only": "ts-node ./scripts/LlmHubStandalone.ts --run",
    "ObjectRecognition:codegen": "ts-node ./scripts/ObjectRecognitionStandalone.ts --codegen",
    "ObjectRecognition:build": "ts-node ./scripts/ObjectRecognitionStandalone.ts --codegen --build",
    "ObjectRecognition:run-only": "ts-node ./scripts/ObjectRecognitionStandalone.ts --run",
    "cache-default-model": "ts-node ./scripts/LlmHubCacheModel.ts",
    "test-mcp-tools": "ts-node ./scripts/TestMcpTools.ts"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@xrpa/xrpa-orchestrator": "^1.14.0",
    "eslint": "8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "@xrpa/xrpa-orchestrator": "^1.14.0"
  },
  "dependencies": {
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.24.5"
  }
}
