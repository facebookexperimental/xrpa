{
  "name": "@xrpa/xred-tracking",
  "version": "1.14.0",
  "description": "Tracking interface",
  "main": "lib/index.js",
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc",
    "codegen": "yarn Fiducials:codegen && yarn OptiTrack:codegen && yarn PulsarSTO:codegen && yarn Quest:codegen",
    "build": "yarn Fiducials:build && yarn OptiTrack:build && yarn PulsarSTO:build && yarn Quest:build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "oss-publish": "tsc && ts-node ./scripts/publish.ts",
    "Fiducials": "ts-node ./scripts/FiducialsStandalone.ts --codegen --run",
    "OptiTrack": "ts-node ./scripts/OptiTrackStandalone.ts --codegen --run",
    "PulsarSTO": "ts-node ./scripts/PulsarSTOStandalone.ts --codegen --run",
    "Quest": "ts-node ./scripts/QuestStandalone.ts --codegen --run",
    "Fiducials:codegen": "ts-node ./scripts/FiducialsStandalone.ts --codegen",
    "Fiducials:build": "ts-node ./scripts/FiducialsStandalone.ts --codegen --build",
    "Fiducials:run-only": "ts-node ./scripts/FiducialsStandalone.ts --run",
    "OptiTrack:codegen": "ts-node ./scripts/OptiTrackStandalone.ts --codegen",
    "OptiTrack:build": "ts-node ./scripts/OptiTrackStandalone.ts --codegen --build",
    "OptiTrack:run-only": "ts-node ./scripts/OptiTrackStandalone.ts --run",
    "PulsarSTO:codegen": "ts-node ./scripts/PulsarSTOStandalone.ts --codegen",
    "PulsarSTO:build": "ts-node ./scripts/PulsarSTOStandalone.ts --codegen --build",
    "PulsarSTO:run-only": "ts-node ./scripts/PulsarSTOStandalone.ts --run",
    "Quest:codegen": "ts-node ./scripts/QuestStandalone.ts --codegen",
    "Quest:build": "ts-node ./scripts/QuestStandalone.ts --codegen --build",
    "Quest:run-only": "ts-node ./scripts/QuestStandalone.ts --run"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@xrpa/xrpa-orchestrator": "^1.14.0",
    "eslint": "8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "@xrpa/xrpa-orchestrator": "^1.14.0"
  }
}
