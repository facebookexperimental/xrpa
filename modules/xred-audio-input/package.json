{
  "name": "@xrpa/xred-audio-input",
  "version": "1.15.0",
  "description": "Audio Input Interface",
  "main": "lib/index.js",
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc",
    "codegen": "ts-node ./scripts/AudioInputStandalone.ts --codegen",
    "codegen:release": "ts-node ./scripts/AudioInputStandalone.ts --codegen --release",
    "build": "ts-node ./scripts/AudioInputStandalone.ts --codegen --build",
    "build:release": "ts-node ./scripts/AudioInputStandalone.ts --codegen --build --release",
    "lint": "eslint --ext .ts,.tsx ./src",
    "oss-publish": "tsc && ts-node ./scripts/publish.ts",
    "AudioInput": "ts-node ./scripts/AudioInputStandalone.ts --codegen --run",
    "AudioInput:run-only": "ts-node ./scripts/AudioInputStandalone.ts --run"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@xrpa/xrpa-orchestrator": "^1.15.0",
    "eslint": "8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "@xrpa/xrpa-orchestrator": "^1.15.0"
  }
}
